public class Foods {

	private String name;
	
	private float price;

	private int count;
	
	/**
	 * 获取属性方法
	 * @return the name
	 */
	public String getName() {
		return name;
	}

	/**
	 * 设置属性方法
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}

	/**
	 * 获取属性方法
	 * @return the price
	 */
	public float getPrice() {
		return price;
	}

	/**
	 * 设置属性方法
	 * @param price the price to set
	 */
	public void setPrice(float price) {
		this.price = price;
	}

	/**
	 * 获取属性方法
	 * @return the count
	 */
	public int getCount() {
		return count;
	}

	/**
	 * 设置属性方法
	 * @param count the count to set
	 */
	public void setCount(int count) {
		this.count = count;
	}
	
}
