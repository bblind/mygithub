public class Boss {

	private String name ;
	
	private String password;
	
	private List<Foods> store;

	/**
	 * 获取属性方法
	 * @return the name
	 */
	public String getName() {
		return name;
	}

	/**
	 * 设置属性方法
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}

	/**
	 * 获取属性方法
	 * @return the password
	 */
	public String getPassword() {
		return password;
	}

	/**
	 * 设置属性方法
	 * @param password the password to set
	 */
	public void setPassword(String password) {
		this.password = password;
	}

	/**
	 * 获取属性方法
	 * @return the store
	 */
	public List<Foods> getStore() {
		return store;
	}

	/**
	 * 设置属性方法
	 * @param store the store to set
	 */
	public void setStore(List<Foods> store) {
		this.store = store;
	}
	
	
}
